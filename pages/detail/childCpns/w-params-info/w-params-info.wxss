/* pages/detail/childCpns/w-params-info/w-params-info.wxss */

.params-info {
  padding: 40rpx 30rpx;
  font-size: 30rpx;
  border-bottom: 12rpx solid #f2f2f2;
}

.row {
  display: flex;
  height: 84rpx;
  line-height: 84rpx;
  border-bottom: 1px solid #f2f2f2;
}

.row .size-item {
  flex: 1;
  margin-right: 42rpx;
}

.row .key {
  width: 160rpx;
}

.row .value {
  flex: 1;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #ff5777;
}
